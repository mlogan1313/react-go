{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
  },
  "rules": {
      "linebreak-style": 0,
      "react/require-default-props": 0,
      "import/no-unresolved": [2, { caseSensitive: false }],
      "jsx-a11y/anchor-is-valid": [ "error", {
            "components": [ "Link" ],
            "specialLink": [ "hrefLeft", "hrefRight", "to" ],
            "aspects": [ "noHref", "invalidHref", "preferButton" ]
          }]
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "settings": {
    "import/resolver": {
      "webpack": {}
    }
  },
}
